<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="412.0" prefWidth="352.0" stylesheets="@../../css/settings.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="example.demo.SettingsController">
   <children>
      <Label fx:id="settingTitle" layoutX="124.0" layoutY="14.0" text="Settings">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <ToggleButton layoutX="57.0" layoutY="107.0" mnemonicParsing="false" text="Dark Mode" />
      <Button fx:id="backtoMainButton" layoutX="14.0" layoutY="32.0" mnemonicParsing="false" onAction="#switchtoMain" prefHeight="15.0" prefWidth="19.0">
         <graphic>
            <ImageView fx:id="backtoMainImg" fitHeight="19.0" fitWidth="19.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../assets/png/arrowback2.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
