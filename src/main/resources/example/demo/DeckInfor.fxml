         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="deleteDeckLabel" layoutX="473.0" layoutY="200.0" text="Delete Deck">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="backButton" layoutX="26.0" layoutY="41.0" mnemonicParsing="false">
         <graphic>
            <ImageView fitHeight="27.0" fitWidth="27.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../assets/png/arrowback2.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</AnchorPane>
