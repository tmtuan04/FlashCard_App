<AnchorPane prefHeight="400.0" prefWidth="571.0" stylesheets="@../../css/deckinfor.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="example.demo.DeckInforController">
   <children>
      <Label fx:id="deckName" alignment="CENTER" layoutY="19.0" prefHeight="18.0" prefWidth="600.0" text="Deck: ">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Pane fx:id="contextPane" layoutX="184.0" layoutY="100.0" prefHeight="200.0" prefWidth="232.0">
         <children>
            <Label fx:id="newLabel" layoutX="23.0" layoutY="30.0" text="New:" />
            <Label fx:id="learnedLabel" layoutX="22.0" layoutY="94.0" text="Learned:" />
            <Label fx:id="dueLabel" layoutX="24.0" layoutY="159.0" text="Due:" />
            <Label fx:id="valueNew" layoutX="167.0" layoutY="30.0" text="Label" />
            <Label fx:id="valueLearned" layoutX="167.0" layoutY="94.0" text="Label" />
            <Label fx:id="valueDue" layoutX="167.0" layoutY="159.0" text="Label" />
